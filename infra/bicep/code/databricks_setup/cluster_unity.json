    "node_type_id": "Standard_E4ds_v4",
    "driver_node_type_id": "Standard_E4ds_v4",
    "ssh_public_keys": [],
    "custom_tags": {},
    "spark_env_vars": {},
    "autotermination_minutes": 30,
    "enable_elastic_disk": true,
    "cluster_source": "UI",
    "init_scripts": [],
    "data_security_mode": "USER_ISOLATION",
    "runtime_engine": "PHOTON"
}