{
    "autoscale": {
        "min_workers": 2,
        "max_workers": 4
    },
    "cluster_name": "openhack_UnityCatalog",
    "spark_version": "11.3.x-scala2.12",
    "spark_conf": {
        "spark.databricks.dataLineage.enabled": "true",
        "spark.databricks.delta.preview.enabled": "true"
    },
    "azure_attributes": {
        "first_on_demand": 1,
        "availability": "ON_DEMAND_AZURE",
        "spot_bid_max_price": -1
